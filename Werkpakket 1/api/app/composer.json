    "illuminate/database": "4.2.*",
    "monolog/monolog": "^1.21",
    "altorouter/altorouter": "1.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*"
  },
  "autoload":{
  "psr-4": {
    "Monolog\\": "src/",
